# Tether Intelligence - AI Computer Agent
# Environment Configuration

# Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL_ID=gemini-2.5-computer-use-preview-10-2025

# VNC Server Configuration
VNC_HOST=localhost
VNC_PORT=5900
VNC_PASSWORD=your_vnc_password_here
VNC_QUALITY=6
VNC_COMPRESSION=2

# Application Configuration
NEXT_PUBLIC_APP_NAME="Tether Intelligence"
NEXT_PUBLIC_VERSION="1.0.0"

# Security
SESSION_SECRET=your_session_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Optional: WebSocket Configuration
WS_PORT=8080
WS_HOST=localhost

# Optional: Database Configuration (if needed)
DATABASE_URL=postgresql://user:password@localhost:5432/tether_intelligence

# Development Configuration
NODE_ENV=development
NEXT_PUBLIC_DEV_MODE=true